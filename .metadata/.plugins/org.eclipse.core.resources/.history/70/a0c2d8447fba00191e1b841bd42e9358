  application:
    name: SpringBootJdbc
  datasource: 
    url: jdbc:mysql://localhost:3306/bootBoarddb
    username: root
    password: 1004
    driver-class-name: com.mysql.jdbc.Driver
    hikari: 
      pool-name: hikari-cp
      maximum-pool-size: 30
      minimum-idle: 2
      data-source-properties: 
          cachePrepStmts: true
          prepStmtCacheSize: 250
          prepStmtCacheSqlLimit: 2048
          useServerPrepStmts: true